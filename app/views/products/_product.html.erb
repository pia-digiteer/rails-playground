<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Qty:</strong>
    <%= product.qty %>
  </p>

  <p>
    <strong>Desc:</strong>
    <%= product.desc %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= product.image_url %>
  </p>

</div>
